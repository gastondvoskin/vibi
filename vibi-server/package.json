{
	"name": "ts-express-server",
	"version": "1.0.0",
	"description": "Express typescript powered server, with paths aliases configuration by default.",
	"main": "src/main.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "tsnd --respawn --rs --cls -r tsconfig-paths/register src/main.ts",
		"build": "tsc && tsc-alias",
		"build-start": "npx prisma generate && npx prisma db push && npm run build && npm start",
		"start": "node dist/src/main.js"
	},
	"license": "ISC",
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.17",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/morgan": "^1.9.4",
		"@types/node": "^20.3.1",
		"@types/node-cron": "^3.0.7",
		"prisma": "^4.16.0",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"tsc-alias": "^1.8.6",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.1.3"
	},
	"dependencies": {
		"@prisma/client": "^4.16.0",
		"@vercel/postgres": "^0.4.1",
		"axios": "^1.4.0",
		"bcrypt": "^5.1.0",
		"cors": "^2.8.5",
		"dotenv": "^16.1.4",
		"express": "^4.18.2",
		"jsonwebtoken": "^9.0.1",
		"morgan": "^1.10.0",
		"node-cron": "^3.0.2",
		"stripe": "^12.14.0"
	}
}
